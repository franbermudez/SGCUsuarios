@*@*model IEnumerable<SGC.Models.Niveles>*@
@model SGC.ViewModels.UsuariosViewModels


<div class="container">
    <h2>Lista de Niveles</h2>
    <table class="table table-striped table-dark">
        <thead>
            <tr>
                <th class="success">Codigo</th>
                <th class="success">Nivel</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var niv in Model.Niveles)
            {
                <tr>
                    <td>@niv.IdNivel</td>
                    <td>@niv.Nivel</td>
                </tr>
            }
        </tbody>


    </table>
</div>
